; HeatAQ Database Configuration EXAMPLE
; Copy this file to /config_heataq/database.env and update with real values
; NEVER commit the actual database.env file!

; Database Connection
DB_HOST=your_database_host
DB_NAME=your_database_name
DB_USER=your_database_user
DB_PASS=your_database_password

; Application Settings
APP_ENV=production
APP_DEBUG=false

; Session Settings
SESSION_LIFETIME=28800
SESSION_NAME=heataq_session

; Security Settings
REQUIRE_AUTH=true
BCRYPT_COST=10

; FROST DNMI (Norwegian Meteorological Institute API)
; Get your client ID from: https://frost.met.no/auth/requestCredentials.html
FROST_CLIENT_ID=your_frost_client_id
