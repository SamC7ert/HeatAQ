; HeatAQ Database Configuration
; Copy this file to database.env and fill in your credentials
; This file should be placed OUTSIDE the public_html directory in production

; Database Connection
DB_HOST=your_database_host
DB_NAME=your_database_name
DB_USER=your_database_user
DB_PASS=your_database_password

; Application Settings
APP_ENV=development
APP_DEBUG=true

; Session Settings
SESSION_LIFETIME=28800
SESSION_NAME=heataq_session

; Security Settings
REQUIRE_AUTH=true
BCRYPT_COST=10
